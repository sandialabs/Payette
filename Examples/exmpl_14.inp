# Example opt
# Title: exmpl_14
# Author: Tim Fuller, tjfulle@sandia.gov
# Keywords: optimization
# Comments: This input file demonstrates the parameter optimization
#           functionality of Payette.

begin simulation exmpl_14
  nowriteprops
  norestart

  begin boundary
    kappa = 0
    tstar = 1.
    ampl = 1
    begin legs
      using time, strain, from columns 1, 3:8
      insert exmpls.tbl
    end legs
  end boundary

  begin material
    constitutive model kayenta
    B0 = 13.0e9
    G0 = 9.86e9
    A1 = 800.e6   # 843.02e6
    A2 = 1.e-10   # 2.731e-10
    A3 = 780.e6   # 821.92e6
    A4 = 1.e-10
    P0 = -1.e99
    CR = 0.001
    RK = .72
    CTI1 = 1.e99
    CTPS = 1.e99
    J3TYPE = 3
  end material

  begin optimization
    method cobyla
    option shearfit
    optimize A1
    optimize A2
    optimize A3
    optimize A4
    gold file shearfit.gold
  end optimization

end simulation
