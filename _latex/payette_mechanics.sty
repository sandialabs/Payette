% -------------------------------------------------------------- mechanics macros

% --
\newcommand{\txte}{\scriptscriptstyle{e}}
\newcommand{\txtp}{\scriptscriptstyle{p}}

% -- invariants
\newcommand{\JTwo}[1][\@empty]{\ifx\@empty#1 {J_2} \else {J_2^{#1}} \fi}
\newcommand{\IOne}[1][\@empty]{\ifx\@empty#1 {I_1} \else {I_1^{#1}} \fi}

% -- stress
\newcommand{\StressSym}{\sigma}
\newcommand{\BackStressSym}{\alpha}
\newcommand{\ShiftedStressSym}{\xi}
\newcommand{\Stress}{\Tensor{\StressSym}{}{}}
\newcommand{\dStress}{\Tensor[\dot]{\StressSym}{}{}}
\newcommand{\ShiftedStress}{\Tensor{\ShiftedStressSym}{}{}}
\newcommand{\DevShiftedStress}{\Tensor{\ShiftedStressSym}{}{\prime}}
\newcommand{\dShiftedStress}{\Tensor[\dot]{\ShiftedStressSym}{}{}}
\newcommand{\BackStress}{\Tensor{\BackStressSym}{}{}}
\newcommand{\dBackStress}{\Tensor[\dot]{\BackStressSym}{}{}}

% -- strain
\newcommand{\StrainSym}{\varepsilon}
\newcommand{\Strain}{\Tensor{\StrainSym}{}{}}
\newcommand{\dStrain}{\Tensor[\dot]{\StrainSym}{}{}}
\newcommand{\DevStrain}{\Tensor{\StrainSym}{}{\prime}}
\newcommand{\dDevStrain}{\Tensor[\dot]{\StrainSym}{}{\prime}}

% -- stretch
\newcommand{\RightStretchSym}{U}
\newcommand{\RightStretch}{\Tensor{\RightStretchSym}{}{}}

% -- plasticity
\newcommand{\FlowDir}{\Tensor{m}{}{}}
\newcommand{\DevFlowDir}{\Tensor{m}{}{\prime}}
\newcommand{\ReturnDir}{\Tensor{p}{}{}}
\newcommand{\YldNormal}{\Tensor{n}{}{}}
\newcommand{\DevYldNormal}{\Tensor{n}{}{\prime}}
\newcommand{\YldStress}{\StressSym_{y}}
\newcommand{\InitYldStress}{\StressSym_{0}}
\newcommand{\dYldStress}{\dot{\StressSym}_{y}}
\newcommand{\ElastStrain}{\Tensor{\StrainSym}{}{\txte}}
\newcommand{\dElastStrain}{\Tensor[\dot]{\StrainSym}{}{\txte}}
\newcommand{\PlastStrain}{\Tensor{\StrainSym}{}{\txtp}}
\newcommand{\dPlastStrain}{\Tensor[\dot]{\StrainSym}{}{\txtp}}
\newcommand{\dDevPlastStrain}{\Tensor[\dot]{\StrainSym}{}{\txtp\prime}}
\newcommand{\Mpsr}{\dot{\lambda}}
\newcommand{\Eqps}{\gamma_{\text{eq}}}
\newcommand{\dIsv}[1]{\dot{q}_{#1}}
\newcommand{\Isv}[1]{q_{#1}}
\newcommand{\IsvModulus}[1]{h_{#1}}
\newcommand{\TensIsv}[1]{\Tensor{q}{#1}{}}
\newcommand{\dTensIsv}[1]{\Tensor[\dot]{q}{#1}{}}
\newcommand{\TensIsvModulus}[1]{\Tensor{h}{#1}{}}
\newcommand{\YldFcn}[1][\@empty]{\ifx\@empty#1 {f} \else {f\left(#1\right)} \fi}
\newcommand{\dYldFcn}[1][\@empty]{\ifx\@empty#1 {\dot{f}} \else {\dot{f}\left(#1\right)} \fi}
\newcommand{\TrialDevShiftedStress}{\Tensor{\ShiftedStressSym}{}{\prime\text{trial}}}
\newcommand{\TrialShiftedStress}{\Tensor{\ShiftedStressSym}{}{\text{trial}}}
\newcommand{\TrialStress}{\Tensor{\StressSym}{}{\text{trial}}}
\newcommand{\dTrialStress}{\Tensor[\dot]{\StressSym}{}{\text{trial}}}
\newcommand{\DPS}{\gamma^{\txtp}}
\newcommand{\dDPS}{\dot{\gamma}^{\txtp}}


% -- elasticities
\newcommand{\BMod}{\kappa}
\newcommand{\SMod}{\mu}
\newcommand{\StiffnessSym}{\mathbb{C}}
\newcommand{\Stiffness}{{\Tensor{\StiffnessSym}{}{}}}



