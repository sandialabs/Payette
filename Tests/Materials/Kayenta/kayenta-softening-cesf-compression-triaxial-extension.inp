begin simulation kayenta-softening-cesf-compression-triaxial-extension
  proportional
  begin material
    constitutive model kayenta
    rho = 2793.0   $ kg/m^3     ## 2.793 g/cm^3  - from Sandia ##
    B0  = 62.59e9  $ Pa ## Derived with elas.py from MatWeb ##
    B1  = 0.0      $ Pa
    B2  = 0.0      $ Pa
    B3  = 0.0      $ Pa
    B4  = 0.0      $ dimensionless
    G0  = 28.0e9   $ Pa ## Derived with elas.py from MatWeb ##
    G1  = 0.0      $ dimensionless
    G2  = 0.0      $ 1 / Pa
    G3  = 0.0      $ Pa
    G4  = 0.0      $ dimensionless
    RJS = 0.0      $ m
    RKS = 0.0      $ Pa / m
    RKN = 0.0      $ Pa / m
    P0  =-1.0e+99  $ Pa / m
    P1  = 0.0      $ 1 /  Pa
    P2  = 0.0      $ 1 /  Pa^2
    P3  = 0.0      $ dimensionless (strain)
    CR  = 0.001    $ dimensionless
    RK  = 1.0      $ dimensionless
    RN  = 0.0      $ Pa
    HC  = 0.0      $ Pa
    CTI1= 1.e99    $ Pa
    CTPS= 1.e99    $ Pa
    T1  = 0.0      $ sec
    T2  = 0.0      $ 1/sec
    T3  = 0.0      $ dimensionless
    T4  = 0.0      $ 1/sec
    T5  = 0.0      $ Pa
    T6  = 0.0      $ sec
    T7  = 0.0      $ 1 / Pa
    J3TYPE = 1     $ Dimensionless
    A2PF   = 0.0
    A4PF   = 0.0
    CRPF   = 0.0
    RKPF   = 0.0
    SUBX   = 0.0

    STRENI  = 1.8706148722e+08 $ ROOTJ2 of yield in tension from MatWeb
    YSLOPEI = 0.0
    PEAKI1I = 1.0e99
    FSLOPEI = 0.0

    STRENF  = 0.1e8
    YSLOPEF = 0.0
    PEAKI1F = 1.0e99
    FSLOPEF = 0.0

    FAIL0 = 0.5    $ TFAIL - not used, but set anyways
    FAIL1 = 10.0   $ FSPEED

    $ The SOFTENING parameter controls what type of softening is used.
    $ SOFTENING = 2 --> Standard Kayenta implementation (FAIL0 and FAIL1)
    $ SOFTENING = 3 --> Constant accumulated strain failure
    $                   FAIL2 = Equivalent Strain at fracture
    $ SOFTENING = 4 --> Johnson-Cook Fracture
    $                   FAIL2 = D1
    $                   FAIL3 = D2
    $                   FAIL4 = D3
    $                   FAIL5 = D4
    $                   FAIL6 = D5
    $                   FAIL7 = reference strain rate
    $                   FAIL8 = melt temperature
    $                   FAIL9 = room temperature
    $ SOFTENING = 5 --> Xue-Wierzbicki Fracture
    $                   FAIL2 = C1
    $                   FAIL3 = C2
    $                   FAIL4 = C3
    $                   FAIL5 = C4
    $                   FAIL6 = N
    SOFTENING = 3


    $ Constant accumulated strain from Wierzbicki2005
    FAIL2 =  0.21 $ From Wierzbicki2005

  end material
  begin boundary
    kappa = 0.
    tfac = 1.
    amplitude = 1
    begin legs
      0, 0.,    0, 444,  0.0,             0.,              0.
      1, 1., 3000, 444, -0.69631*0.2e10, -0.69631*0.2e10, -0.17407*0.2e10
    end legs
  end boundary
end simulation
